programa {
  funcao inicio() {

    // Declaração de variáveis
    real valorAluguel
    real valorMulta = 120.50
    real valorFinalAluguel

    // Entrada de Dados
    escreva("Digite o valor do aluguel (R$): ")
    leia(valorAluguel)

    // Processamento, Cálculo, Lógica
    valorFinalAluguel = valorAluguel + valorMulta

    // Saída de Dados
    escreva("O valor total a pagar é: ", valorFinalAluguel)

  }
}
